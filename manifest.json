{
  "name": "+1s",
  "author": "WangZhe",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/contextMenus.js"
    ]
  },
  "browser_action": {
    "default_title": "Some tooltip",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      //这个坑　我要哭了
      "matches": [
        "<all_urls>"
        //"*://jiaowu.swjtu.edu.cn/service/login.jsp*",
        //"*://jiaowu.swjtu.edu.cn//service/login.jsp*",
        //"*://jiaowu.swjtu.edu.cn/servlet/UserLoginSQLAction",
        //"*://jiaowu.swjtu.edu.cn//servlet/UserLoginSQLAction",
        //"*://dean.swjtu.edu.cn/service/login.jsp*"
        //"*://service.swjtu.edu.cn:8080/selfservice*"
        //"*://dean.swjtu.edu.cn/servlet/UserGenearchLoginAction"
      ],
      "js": [
        "js/libmath.js",
        "js/libimage.js",
        "js/libjson.js",
        "js/student.js",
        "js/genearch.js",
        "js/network.js",
        "js/dispatch.js"
      ]
    }
  ],
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "icons": {
    "48": "images/icon.png"
  },
  "web_accessible_resources": [
    "data/*.*"
  ]
}

